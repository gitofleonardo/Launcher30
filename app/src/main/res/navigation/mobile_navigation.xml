    <fragment
        android:id="@+id/navigation_customization"
        android:name="com.hhvvg.launcher3customizer.ui.customize.CustomizeFragment"
        android:label="@string/title_customize"
        tools:layout="@layout/fragment_customize" />

    <fragment
        android:id="@+id/navigation_more_settings"
        android:name="com.hhvvg.launcher3customizer.ui.more.MoreSettingsFragment"
